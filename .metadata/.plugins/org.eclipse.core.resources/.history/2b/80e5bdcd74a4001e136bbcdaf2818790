package String_Char;

import java.util.Scanner;

public class Test01 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		String a = sc.next();
		String b = sc.next();
		String answer;
		
		System.out.println(a+b);
		
		System.out.println("//////////////");
		
		answer=a.concat(b);
		
		System.out.println(answer);
		
	}

}
