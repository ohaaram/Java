public class DataTypeTest {
	
	public static void main(String[] args) {
		//정수형
		byte num1=127;
		short num2=32767;
		int num3=217483647;
		long num4=922337203685477507L;
		
		System.out.println("num1 : "+num1);//컨트롤+알트+방향이 밑으로하면 해당 줄을 복사
		System.out.println("num2 : "+num2);//컨트롤+알트+방향이 밑으로하면 해당 줄을 복사
		System.out.println("num3 : "+num3);//컨트롤+알트+방향이 밑으로하면 해당 줄을 복사
		System.out.println("num4 : "+num4);//컨트롤+알트+방향이 밑으로하면 해당 줄을 복사

		
		//실수형
		
		float var1 = 0.123456789f;
		double 
		
		//논리형
		
		//문자형
		
		//문자열
	}
}
