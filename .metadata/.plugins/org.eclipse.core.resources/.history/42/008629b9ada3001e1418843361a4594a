package sub2;

/*
 * 날짜 : 2023/12/26
 * 이름 : 오아람
 * 내용 : Java 자료형 실습하기
 */
public class DataTypeTest {
	
	public static void main(String[] args) {
		//정수형
		byte num1=127;
		short num2=32767;
		int num3=217483647;//많이 씀
		long num4=922337203685477507L;
		
		System.out.println("num1 : "+num1);//컨트롤+알트+방향이 밑으로하면 해당 줄을 복사
		System.out.println("num2 : "+num2);//컨트롤+알트+방향이 밑으로하면 해당 줄을 복사
		System.out.println("num3 : "+num3);//컨트롤+알트+방향이 밑으로하면 해당 줄을 복사
		System.out.println("num4 : "+num4);//컨트롤+알트+방향이 밑으로하면 해당 줄을 복사

		
		//실수형
		
		float var1 = 0.123456789f;//소수점 8자리
		double var2 = 0.123456789123456789;//많이 씀,소수점 17자리
		
		System.out.println("var1 : "+var1);
		System.out.println("var2 : "+var2);
		
		//논리형
		boolean valu1=true;
		boolean valu2=false;
		
		//문자형
		
		//문자열
	}
}
